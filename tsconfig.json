{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2016",
    "module": "commonjs",
    "lib": ["es2016", "ESNext.AsyncIterable"],
    "checkJs": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "tsBuildInfoFile": "./",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
